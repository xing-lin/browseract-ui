{
  "name": "browseract-ui",
  "version": "1.7.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "style": "dist/index.css",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite",
    "check": "npm run test:coverage && npm run build && npm run build-storybook",
    "prebuild": "rm -rf dist/",
    "build": "npm run rollup",
    "rollup": "rollup -c --bundleConfigAsCjs",
    "lint": "eslint src/**/*.{ts,tsx}",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "release": "semantic-release",
    "prepare": "husky"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-dom": ">=19"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "4.1.1",
    "@eslint/js": "9.37.0",
    "@rollup/plugin-commonjs": "28.0.6",
    "@rollup/plugin-node-resolve": "16.0.2",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "12.1.4",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "12.0.2",
    "@semantic-release/release-notes-generator": "14.1.0",
    "@storybook/addon-a11y": "9.1.10",
    "@storybook/addon-docs": "9.1.10",
    "@storybook/addon-vitest": "9.1.10",
    "@storybook/react-vite": "9.1.10",
    "@storybook/testing-library": "0.2.2",
    "@svgr/rollup": "8.1.0",
    "@tailwindcss/postcss": "4.1.16",
    "@tailwindcss/vite": "4.1.16",
    "@testing-library/react": "16.3.0",
    "@types/node": "24.7.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1",
    "@vitejs/plugin-react": "5.0.4",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "clsx": "2.1.1",
    "eslint": "9.37.0",
    "eslint-plugin-react-hooks": "7.0.0",
    "eslint-plugin-react-refresh": "0.4.23",
    "eslint-plugin-storybook": "9.1.10",
    "globals": "16.4.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.4",
    "markdown-to-jsx": "7.7.15",
    "playwright": "1.56.0",
    "postcss": "8.5.6",
    "postcss-import": "16.1.1",
    "prettier": "3.6.2",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "rollup": "4.52.4",
    "rollup-plugin-dts": "6.2.3",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss": "4.0.2",
    "semantic-release": "24.2.9",
    "storybook": "9.1.10",
    "tailwind-merge": "3.3.1",
    "tailwindcss": "4.1.16",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.0",
    "vite": "npm:rolldown-vite@7.1.16",
    "vite-plugin-svgr": "4.5.0",
    "vitest": "3.2.4"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.16"
  },
  "keywords": [
    "browseract-ui",
    "browseract",
    "react",
    "react-ui",
    "react-components",
    "ui-components"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xing-lin/browseract-ui.git"
  },
  "author": "Xing Lin <linxing0427@163.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "description": "BrowserAct React Component Library",
  "lint-staged": {
    "src/**/*.{ts,tsx}": "eslint --fix",
    "*.{js,ts,jsx,tsx,css,scss,md,json}": "prettier --write"
  }
}
